{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "incremental": true,
    "composite": true,
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "jsx": "react-jsx",
    "baseUrl": "."
  },
  "include": [
    "src/**/*",
    "gatsby/**/*"
  ]
}
